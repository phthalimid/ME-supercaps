@article{rusling_electrochemical_1997,
  title = {Electrochemical Catalysis in Microemulsions. {{Dynamics}} and Organic {{synthesis1Dedicated}} to {{Prof}}. {{K}}.{{B}}. {{Oldham}} on the Occasion of His Retirement from {{Trent University}}.1},
  author = {Rusling, James F. and Zhou, De-Ling},
  year = {1997},
